  <%= form_with url: search_path ,method: :get do |f| %>
  <%= f.text_field :content %>
  <%= f.select :model,options_for_select({"User"=>"User","Book"=>"Book"}) %>
  <%= f.select :method,options_for_select({"完全一致"=>"perfect","部分一致"=>"Partially","前方一致"=>"front","後方一致"=>"back"}) %>
  <%= f.submit'検索' %>
  <% end %>
